
{{#partial "content"}}
    {{#if rangers}}
        <h2 class="text-center">Our Rangers</h2>
        <ul class="list-group">
            {{#each rangers}}
                <li class="list-group-item"><a href="/rangers/{{id}}/details">{{name}}</a></li>
            {{/each}}
        </ul>
    {{else}}
        <h5 class="text-center">No rangers registered. Report a sighting to register yourself as a ranger</h5>
    {{/if}}

{{/partial}}

{{> layout.hbs}}